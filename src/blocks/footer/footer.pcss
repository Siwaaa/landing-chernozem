.footer {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16px;
  font-weight: 200;
  color: #898989;
  margin-bottom: 20px;

  &__social {
    margin-right: 20px;
    margin-left: 20px;

    & > a {
      &:first-child {
        margin-right: 15px;
      }
      & img {
        width: 30px;
        height: 30px;
        object-fit: contain;
      }
    }
  }
  &__label {
    
  }
}